package controlStttest;
import java.util.*;
public class EvenOddarray {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		System.out.println("Enter total number count of array : ");
		int n=sc.nextInt();
		System.out.println("Great ! Enter "+n+" integers: ");
		
		
		int arr[]=new int[n];
		for( int i=0;i<n;i++) {
			arr[0]=sc.nextInt();
			
		}
		for(int i=0;i<n;i++) {
			System.out.println(arr[i]);
		}
		
	}

}
