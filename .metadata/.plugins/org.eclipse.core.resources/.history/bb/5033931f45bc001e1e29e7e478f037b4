import java.util.*;
public class arraySorting {

	public static void main(String[] args) {
		System.out.println("Enter total number of array :");
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		
		int arr[]=new int[n];
		
		for(int i=0;i<n;i++) {
			 arr[n]=sc.nextInt();
		}
		
	}

}
