import java.util.*;
public class arraySorting {

	public static void main(String[] args) {
		System.out.println("Enter total number of array :");
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		
		System.out.println("Enter array "+n+ " Element ");
		int arr[]=new int[n];
		
		for(int i=0;i<n;i++) {
			 arr[i]=sc.nextInt();
		}
		int maxnumber=arr[0];
		int minnumber=arr[0];
		for(int i=0;i<n;i++) {
			if(arr[i]<arr[i+1]) {
				maxnumber=arr[i+1];
				
			}
		}
		for(int i=0;i<n;i++) {
			if(arr[i]>arr[i+1]) {
				minnumber=arr[i+1];
				
			}
		}
		
	}

}
