
public class ArraySwaping {
	    public static void main(String args[])
	    {
	        int[] arr = {7,0,1,6,5 };
	       
	
	        int j=0;
	        for(int i=arr.length-1;i>=arr.length/2;i--) {
	     
	        	int val=arr[i];
	             j=val;
	            System.out.println(arr[j]+" ");
	        	j++;
	        	
	        }
	    
	        for(int i:arr) {
	        	System.out.print(i+" ");
	        }
	        
	    }
	

}
